{
  "name": "react-full-stack-starter-server",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect-sqlite3": "^0.9.10",
    "cookie-parser": "^1.4.3",
    "express": "4.13.3",
    "express-session": "^1.15.6",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "sdk": "^0.5.0",
    "socket.io": "^2.0.4"
  },
  "scripts": {
    "postinstall": "cd client && npm install",
    "start": "node build/index.js",
    "watch": "babel-watch server/index.js",
    "dev": "concurrently -c blue,yellow \"npm run watch\" \"cd client && npm start\"",
    "build": "concurrently \"babel server -d build\" \"cd client && npm test && npm run build\"",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-env": "^1.6.1",
    "babel-watch": "^2.0.6",
    "concurrently": "^3.5.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1"
  }
}
